<div id="soundplayer">
  <div class="player">
    <div class="closePlayer"><a href="#">&times;</a></div>

    <div class="controls">
      <a class="prev"></a>
      <a class="play"></a>
      <a class="pause"></a>
      <a class="next"></a>
    </div>

    <div class="playlist">
      <ol class="tracks"></ol>
    </div>

    <div class="title-bar">
      <div class="time">
        <span class="ran">0.00</span>&nbsp;/&nbsp;<span class="total">0.00</span>
      </div>

      <div class="trackNumber"></div>

      <div class="title clearfix">
        <div class="artistName"></div>
        <div class="trackName"></div>
      </div>
    </div>

    <div class="waveform">
      <div class="complete"></div>
      <div class="ran"></div>
      <div class="wave"></div>
    </div>
  </div>
</div>